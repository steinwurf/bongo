{% for file in files %}
{% cycle '<div class = row>' '' '' %}
    <div class="col-sm-4 col-md-4">
        <div class="thumbnail">
            <a href="/{{file.path}}" class="thumbnail">
                <img src="/static/{{file.path}}" alt="{{file.name}}" style="max-height:300px">
            </a>
            <div class="caption">
                <p>{{file.name}}</p>
            </div>
        </div>
    </div>
{% cycle '' '' '</div>' %}
{% endfor %}